<% 
    var pageList = [],
        i;
    page.posts.each(function(item) {
        pageList.push(item);
    });
%>
<% for (i = 0; i < pageList.length; i++) { %>
    <%- partial('_partial/simple_article', {item: pageList[i], index: true, showEntry: true, showTags: true}) %>
    <% if(i != pageList.length - 1) { %>
    <hr>
    <% } %>
<% } %>


<%- partial('_partial/pagination') %>