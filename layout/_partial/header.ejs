<nav class="indigo">
    <div class="nav-wrapper">
        <div class="container">
            <a href="/" class="brand-logo hide-on-small-only"><%= config.title %></a>
            <a href="#" data-activates="main-menu" class="button-collapse">
                <i class="fa fa-navicon"></i>
            </a>
            <ul class="right hide-on-med-and-down">
                <% for (var i in theme.menu){ %>
                    <li>
                        <a class="<%= theme.menu[i].category ? 'category-menu' : '' %>" href="<%= theme.menu[i].link %>" <% if (theme.menu[i].category) { %>data-activates="category-menu" <% } %>>
                            <i class="fa fa-<%= theme.menu[i].icon %> "></i>
                            <%= theme.menu[i].text %>
                        </a>
                    </li>
                <% } %>
            </ul>
            <%- partial('side_nav') %>
        </div>
    </div>
</nav>
